<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefWidth="500.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.petrik.etlap.etlap.controllers.KategoriaMainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <HBox prefHeight="50.0" prefWidth="200.0">
        <children>
            <Button maxWidth="150.0" onAction="#onHozzaadasButton" prefWidth="125.0" text="Hozzáadás">
                <HBox.margin>
                    <Insets right="10.0" />
                </HBox.margin>
                <font>
                    <Font size="14.0" />
                </font></Button>
            <Button maxWidth="150.0" mnemonicParsing="false" onAction="#onTorlesButton" prefWidth="100.0" text="Törlés">
                <font>
                    <Font size="14.0" />
                </font></Button>
        </children>
    </HBox>
    <TableView fx:id="kategoriaTable" maxHeight="400.0" maxWidth="500.0" prefHeight="200.0" prefWidth="500.0">
        <columns>
            <TableColumn fx:id="kategoriaNev" maxWidth="500.0" prefWidth="470.0" text="Név" />
        </columns>
    </TableView>

</VBox>
